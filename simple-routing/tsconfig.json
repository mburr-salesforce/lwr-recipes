{
    "compilerOptions": {
        "rootDir": "src",
        "outDir": "build",
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "target": "es2019",
        "module": "commonjs",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "example/home": [
                "src/modules/example/home/home.ts"
            ],
            "example/homePageHandler": [
                "src/modules/example/homePageHandler/homePageHandler.ts"
            ],
            "example/link": [
                "src/modules/example/link/link.ts"
            ],
            "example/namedPageHandler": [
                "src/modules/example/namedPageHandler/namedPageHandler.ts"
            ],
            "example/nav": [
                "src/modules/example/nav/nav.ts"
            ],
            "example/navItem": [
                "src/modules/example/navItem/navItem.ts"
            ],
            "example/recipes": [
                "src/modules/example/recipes/recipes.ts"
            ],
            "example/recipesItem": [
                "src/modules/example/recipesItem/recipesItem.ts"
            ],
            "example/recipesPageAttributeApplier": [
                "src/modules/example/recipesPageAttributeApplier/recipesPageAttributeApplier.ts"
            ],
            "example/recipesPageHandler": [
                "src/modules/example/recipesPageHandler/recipesPageHandler.ts"
            ],
            "example/rootRouter": [
                "src/modules/example/rootRouter/rootRouter.ts"
            ],
            "lwr/router": [
                "node_modules/@lwrjs/router/build/modules/lwr/router/router.d.ts"
            ],
            "lwr/navigation": [
                "node_modules/@lwrjs/router/build/modules/lwr/navigation/navigation.d.ts"
            ],
            "lwr/navigationContext": [
                "node_modules/@lwrjs/router/build/modules/lwr/navigationContext/navigationContext.d.ts"
            ],
            "lwr/navigationService": [
                "node_modules/@lwrjs/router/build/modules/lwr/navigationService/navigationService.d.ts"
            ]
        },
    }
}