{
    "name": "security",
    "version": "0.11.0-alpha.6",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/src/index.d.ts",
    "main": "build/src/index.js",
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "MODE=dev node build/src/index.js",
        "lwr:build": "lwr build --clean",
        "start": "MODE=prod node build/src/index.js",
        "dev:compat": "MODE=compat node build/src/index.js",
        "start:prod-compat": "MODE=prod-compat node build/src/index.js",
        "test": "wdio wdio.conf.ts",
        "test:debug": "DEBUG=true wdio wdio.conf.ts"
    },
    "devDependencies": {
        "@lwrjs/security": "0.11.0-alpha.6",
        "@wdio/cli": "^8.10.4",
        "@wdio/globals": "^8.10.4",
        "@wdio/local-runner": "^8.10.4",
        "@wdio/mocha-framework": "^8.10.4",
        "lwc": "3.1.2",
        "lwr": "0.11.0-alpha.6",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webdriverio": "^8.10.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "node": "16.20.1",
        "yarn": "1.22.19"
    }
}
