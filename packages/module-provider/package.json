{
    "name": "module-provider",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "type": "commonjs",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js",
        "pageObjects"
    ],
    "scripts": {
        "build": "tsc -b && yarn utam:compile",
        "clean": "rm -rf build node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat",
        "test": "wdio ./src/config/wdio.conf.ts",
        "utam:compile": "utam -c utam.config.cjs"
    },
    "devDependencies": {
        "@types/jquery": "^3.5.16",
        "@wdio/cli": "^7.13.2",
        "@wdio/local-runner": "^7.13.2",
        "@wdio/mocha-framework": "^7.13.2",
        "chromedriver": "^111.0.0",
        "lwc": "2.40.0",
        "lwr": "0.9.0",
        "simple-routing": "0.0.1",
        "ts-node": "^10.1.0",
        "typescript": "^4.9.5",
        "utam": "^1.7.0",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-utam-service": "^1.7.0"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "volta": {
        "node": "16.19.1",
        "yarn": "1.22.19"
    }
}
