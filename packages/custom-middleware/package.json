{
    "name": "custom-middleware",
    "version": "0.11.0-alpha.13",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/es/index.d.ts",
    "main": "build/es/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
        "clean": "rm -rf build && rm -rf __lwr_cache__ node_modules tsconfig.tsbuildinfo",
        "build": "tsc -b",
        "dev": "node build/es/src/express.js",
        "start:express": "node build/es/src/express.js",
        "start:koa": "node build/es/src/koa.js",
        "start": "MODE=prod node build/es/src/express.js",
        "start:compat": "MODE=compat node build/es/src/express.js",
        "start:prod-compat": "MODE=prod-compat node build/es/src/express.js",
        "test": "wdio wdio.conf.ts",
        "test:debug": "DEBUG=true wdio wdio.conf.ts"
    },
    "dependencies": {
        "lwc": "4.0.0",
        "lwr": "0.11.0-alpha.13"
    },
    "devDependencies": {
        "@wdio/cli": "8.13.13",
        "@wdio/globals": "8.13.13",
        "@wdio/local-runner": "8.13.13",
        "@wdio/mocha-framework": "8.13.13",
        "chromedriver": "^117.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "wdio-chromedriver-service": "^8.1.1",
        "webdriverio": "8.13.13"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "volta": {
        "node": "18.18.2",
        "yarn": "1.22.19"
    }
}
