{
    "name": "custom-middleware",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
        "clean": "rm -rf build && rm -rf __lwr_cache__ tsconfig.tsbuildinfo",
        "build": "tsc -b",
        "dev": "node build/src/express.js",
        "start:express": "node build/src/express.js",
        "start:koa": "node build/src/koa.js",
        "start": "MODE=prod node build/src/express.js",
        "start:compat": "MODE=compat node build/src/express.js",
        "start:prod-compat": "MODE=prod-compat node build/src/express.js",
        "test": "wdio wdio.conf.ts",
        "test:debug": "DEBUG=true wdio wdio.conf.ts"
    },
    "devDependencies": {
        "@wdio/cli": "^8.10.4",
        "@wdio/globals": "^8.10.4",
        "@wdio/local-runner": "^8.10.4",
        "@wdio/mocha-framework": "^8.10.4",
        "chromedriver": "^112.0.0",
        "lwc": "2.40.0",
        "lwr": "^0.9.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "wdio-chromedriver-service": "^8.1.1",
        "webdriverio": "^8.10.4"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "volta": {
        "node": "16.19.1",
        "yarn": "1.22.19"
    }
}
