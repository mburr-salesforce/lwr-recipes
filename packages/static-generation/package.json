{
    "name": "static-generation",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
