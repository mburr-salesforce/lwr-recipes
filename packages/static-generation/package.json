        "@lwrjs/types": "0.8.8",
        "rimraf": "^3.0.2"
    },
    "dependencies": {
        "lwc": "2.17.0",
        "lwr": "0.8.8",
        "serve": "^13.0.0"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "volta": {
        "node": "16.19.1",
        "yarn": "1.22.19"
    }
}
