{
    "name": "static-generation",
    "version": "0.10.2",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules __lwr_cache__ site __generated_site__ tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start:static": "serve site",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat"
    },
    "devDependencies": {
        "@lwrjs/types": "0.10.2"
    },
    "dependencies": {
        "lwc": "3.1.2",
        "lwr": "0.10.2",
        "serve": "^14.2.0",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "node": "16.20.1",
        "yarn": "1.22.19"
    }
}
