{
    "name": "nested-routing",
    "version": "0.11.0-alpha.6",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start --mode=prod-compat",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat",
        "test": "wdio wdio.conf.ts",
        "test:debug": "DEBUG=true wdio wdio.conf.ts"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-typescript": "^7.21.0",
        "@wdio/cli": "^8.10.4",
        "@wdio/globals": "^8.10.4",
        "@wdio/local-runner": "^8.10.4",
        "@wdio/mocha-framework": "^8.10.4",
        "lwc": "3.1.2",
        "lwr": "0.11.0-alpha.6",
        "typescript": "^4.9.5",
        "webdriverio": "^8.10.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "node": "16.20.1",
        "yarn": "1.22.19"
    }
}
