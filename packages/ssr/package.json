{
    "name": "ssr",
    "version": "0.10.0",
    "license": "MIT",
    "private": true,
    "files": [
        "build/**/*.css",
        "build/**/*.html",
        "build/**/*.js"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat"
    },
    "devDependencies": {
        "@lwrjs/lwc-ssr": "0.10.0",
        "@lwrjs/types": "0.10.0",
        "lwc": "3.0.0",
        "lwr": "0.10.0",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "node": "16.20.1",
        "yarn": "1.22.19"
    }
}
