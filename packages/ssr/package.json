{
    "name": "ssr",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "files": [
        "build/**/*.css",
        "build/**/*.html",
        "build/**/*.js"
    ],
    "scripts": {
        "clean": "rm -rf build __lwr_cache__ __generated_site__",
        "build": "tsc -b",
        "build:static": "LEGACY_LOADER=true echo \"Pre-building the SSR app...\" && lwr static-build --mode=prod-compat",
        "start:static": "MODE=prod-compat LEGACY_LOADER=true SSR_STATIC_BUNDLES=true node scripts/start-static.mjs",
        "dev": "lwr serve",
        "start": "lwr serve --mode prod",
        "start:compat": "lwr serve --mode compat",
        "start:prod-compat": "lwr serve --mode prod-compat"
    },
    "devDependencies": {
        "@lwrjs/lwc-ssr": "0.9.0-alpha.12",
        "@lwrjs/types": "0.9.0-alpha.12",
        "lwc": "2.24.0",
        "lwr": "0.9.0-alpha.12",
        "typescript": "^4.2.2"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "volta": {
        "node": "16.19.1",
        "yarn": "1.22.19"
    }
}
