{
    "name": "ssr",
    "version": "0.11.0-alpha.7",
    "license": "MIT",
    "private": true,
    "files": [
        "build/**/*.css",
        "build/**/*.html",
        "build/**/*.js"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat"
    },
    "devDependencies": {
        "@lwrjs/lwc-ssr": "0.11.0-alpha.7",
        "@lwrjs/types": "0.11.0-alpha.7",
        "lwc": "3.1.2",
        "lwr": "0.11.0-alpha.7",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "volta": {
        "node": "18.18.2",
        "yarn": "1.22.19"
    }
}
