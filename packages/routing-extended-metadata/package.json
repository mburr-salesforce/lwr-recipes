{
    "name": "routing-extended-metadata",
    "version": "0.11.0-alpha.13",
    "license": "MIT",
    "private": true,
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js",
        "pageObjects"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build pageObjects/ node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat"
    },
    "dependencies": {
        "lwc": "3.1.2",
        "lwr": "0.11.0-alpha.13"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-typescript": "^7.21.0",
        "@babel/register": "^7.21.0",
        "@types/jest": "^25.2.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "volta": {
        "node": "18.18.2",
        "yarn": "1.22.19"
    }
}
