{
    "extends": "../../tsconfig.settings.json",
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": "src",
        "outDir": "build/cjs",
        "module": "commonjs",
        "paths": {
            "example/contextProvider": ["src/modules/example/contextProvider/contextProvider.ts"],
            "example/contextUtils": ["src/modules/example/contextUtils/contextUtils.ts"],
            "example/currentRouteMetadata": [
                "src/modules/example/currentRouteMetadata/currentRouteMetadata.ts"
            ],
            "example/metadataPageHandler": ["src/modules/example/metadataPageHandler/metadataPageHandler.ts"],
            "example/link": ["src/modules/example/link/link.ts"],
            "example/namedPageHandler": ["src/modules/example/namedPageHandler/namedPageHandler.ts"],
            "example/nav": ["src/modules/example/nav/nav.ts"],
            "example/navItem": ["src/modules/example/navItem/navItem.ts"],
            "example/metadataItem": ["src/modules/example/metadataItem/metadataItem.ts"],
            "example/metadataPageAttributeApplier": [
                "src/modules/example/metadataPageAttributeApplier/metadataPageAttributeApplier.ts"
            ],
            "example/rootRouter": ["src/modules/example/rootRouter/rootRouter.ts"],
            "lwr/router": [
                "../../node_modules/@lwrjs/router/build/modules/lwr/router/router.d.ts",
                "../../node_modules/@lwrjs/router/build/es/modules/lwr/router/router.d.ts"
            ],
