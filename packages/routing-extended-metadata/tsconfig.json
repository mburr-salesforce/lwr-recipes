                "../../node_modules/@lwrjs/router/build/es/modules/lwr/navigation/navigation.d.ts"
            ],
            "lwr/navigationContext": [
                "../../node_modules/@lwrjs/router/build/modules/lwr/navigationContext/navigationContext.d.ts",
                "../../node_modules/@lwrjs/router/build/es/modules/lwr/navigationContext/navigationContext.d.ts"
            ],
            "lwr/navigationService": [
                "../../node_modules/@lwrjs/router/build/modules/lwr/navigationService/navigationService.d.ts",
                "../../node_modules/@lwrjs/router/build/es/modules/lwr/navigationService/navigationService.d.ts"
            ]
        }
    },
    "exclude": ["build", "pageObjects"]
}
