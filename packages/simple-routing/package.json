{
    "name": "simple-routing",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js",
        "pageObjects"
    ],
    "scripts": {
        "clean": "rimraf build/ pageObjects/ node_modules/ __lwr_cache__",
        "build": "tsc -b && yarn utam:compile",
        "dev": "node build/autostart.js",
        "start": "MODE=prod yarn dev",
        "start:compat": "MODE=compat yarn dev",
        "start:prod-compat": "MODE=prod-compat yarn dev",
        "utam:compile": "utam -c utam.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@types/jest": "^25.2.1",
        "chromedriver": "^100.0.0",
        "lerna": "^3.20.2",
        "lwc": "2.17.0",
        "lwr": "0.7.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.2.2",
        "utam": "1.0.1"
    },
    "engines": {
        "node": ">=14.16 <17"
    }
}
