{
    "name": "simple-routing",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js",
        "pageObjects"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build pageObjects/ node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "test": "wdio wdio.conf.ts",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-typescript": "^7.21.0",
        "@babel/register": "^7.21.0",
        "@types/jest": "^25.2.1",
        "@wdio/cli": "^8.10.4",
        "@wdio/globals": "^8.10.4",
        "@wdio/local-runner": "^8.10.4",
        "@wdio/mocha-framework": "^8.10.4",
        "chromedriver": "^112.0.0",
        "lwc": "2.40.0",
        "lwr": "^0.9.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "wdio-chromedriver-service": "^8.1.1",
        "webdriverio": "^8.10.4"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "volta": {
        "node": "16.19.1",
        "yarn": "1.22.19"
    }
}
