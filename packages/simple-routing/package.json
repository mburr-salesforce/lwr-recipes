{
    "name": "simple-routing",
    "version": "0.11.0-alpha.7",
    "license": "MIT",
    "private": true,
    "type": "module",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "files": [
        "build/**/*.js",
        "pageObjects"
    ],
    "scripts": {
        "build": "tsc -b",
        "clean": "rm -rf build pageObjects/ node_modules __lwr_cache__ site tsconfig.tsbuildinfo",
        "dev": "lwr dev",
        "lwr:build": "lwr build --clean",
        "start": "lwr start",
        "stage": "yarn lwr:build && yarn start",
        "stage:prod-compat": "yarn lwr:build --mode prod-compat && yarn start --mode prod-compat",
        "test": "wdio wdio.conf.ts",
        "test:debug": "DEBUG=true wdio wdio.conf.ts"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-typescript": "^7.21.0",
        "@babel/register": "^7.21.0",
        "@types/jest": "^25.2.1",
        "@wdio/cli": "8.13.13",
        "@wdio/globals": "8.13.13",
        "@wdio/local-runner": "8.13.13",
        "@wdio/mocha-framework": "8.13.13",
        "chromedriver": "^117.0.3",
        "lwc": "3.1.2",
        "lwr": "0.11.0-alpha.7",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "wdio-chromedriver-service": "^8.1.1",
        "webdriverio": "8.13.13"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "volta": {
        "node": "16.20.1",
        "yarn": "1.22.19"
    }
}
